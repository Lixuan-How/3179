{
    "$schema": "https://vega.github.io/schema/vega-lite/v5.json",
    "title": { "text": "Population Density in 2022 of overall ethnicities and age" },
    "data": {
        "url": "https://raw.githubusercontent.com/Lixuan-How/3179/refs/heads/main/Project%202/Data/temporary_normalised_population_2022.csv"
    },
    "transform": [
    {"calculate": "(datum.population_thousands*1000)/datum.land_size_sq_km", "as": "Density"}
],
    "mark": "bar",
    "encoding": {
        "x": { "field": "population_density", "type": "quantitative"},
        "y": { "field": "state", "type": "nominal", "title": null},
    "tooltip": [
    {"field": "state", "type": "nominal", "title": "State"},
    {"field": "land_size_sq_km", "type": "quantitative", "title": "Land Size (kmÂ²)", "format": ".2f"},
    {"field": "population_thousands", "type": "quantitative", "title": "Population (thousands)", "format": ".2f"},
    {
        "field": "Density",
        "type": "quantitative",
        "title": "Population number per km^2",
        "format": ".2f"
    }
    ]
    }
}